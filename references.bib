@inproceedings{smooth-gan,
	address = {Cham},
	series = {Lecture {Notes} in {Computer} {Science}},
	title = {{SMOOTH}-{GAN}: {Towards} {Sharp} and {Smooth} {Synthetic} {EHR} {Data} {Generation}},
