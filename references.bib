	number = {4},
	journal = {Obstetrics and Gynecology},
	shortjournal = {Obstet Gynecol},
	author = {Grobman, William A. and Lai, Yinglei and Landon, Mark B. and Spong, Catherine Y. and Leveno, Kenneth J. and Rouse, Dwight J. and Varner, Michael W. and Moawad, Atef H. and Caritis, Steve N. and Harper, Margaret and Wapner, Ronald J. and Sorokin, Yoram and Miodovnik, Menachem and Carpenter, Marshall and O'Sullivan, Mary J. and Sibai, Baha M. and Langer, Oded and Thorp, John M. and Ramin, Susan M. and Mercer, Brian M. and {National Institute of Child Health and Human Development (NICHD) Maternal-Fetal Medicine Units Network (MFMU)}},
	year = {2007},
	pmid = {17400840},
	keywords = {Adolescent, Adult, Body Mass Index, Contraindications, Female, Forecasting, Humans, Maternal Age, Nomograms, notion, Pregnancy, Pregnancy Complications, Racial Groups, Retrospective Studies, {ROC} Curve, Trial of Labor, Vaginal Birth after Cesarean},
	file = {Grobman et al_2007_Development of a nomogram for prediction of vaginal birth after cesarean.pdf:/Users/joaoalmeida/Zotero/storage/DFMJREE7/Grobman et al_2007_Development of a nomogram for prediction of vaginal birth after cesarean.pdf:application/pdf},
}


@article{ullah_reliable_2021,
	title = {Reliable Prediction Models Based on Enriched Data for Identifying the Mode of Childbirth by Using Machine Learning Methods: Development Study},
	volume = {23},
	issn = {1438-8871},
	doi = {10.2196/28856},
	shorttitle = {Reliable Prediction Models Based on Enriched Data for Identifying the Mode of Childbirth by Using Machine Learning Methods},
	abstract = {{BACKGROUND}: The use of artificial intelligence has revolutionized every area of life such as business and trade, social and electronic media, education and learning, manufacturing industries, medicine and sciences, and every other sector. The new reforms and advanced technologies of artificial intelligence have enabled data analysts to transmute raw data generated by these sectors into meaningful insights for an effective decision-making process. Health care is one of the integral sectors where a large amount of data is generated daily, and making effective decisions based on these data is therefore a challenge. In this study, cases related to childbirth either by the traditional method of vaginal delivery or cesarean delivery were investigated. Cesarean delivery is performed to save both the mother and the fetus when complications related to vaginal birth arise.
{OBJECTIVE}: The aim of this study was to develop reliable prediction models for a maternity care decision support system to predict the mode of delivery before childbirth.
{METHODS}: This study was conducted in 2 parts for identifying the mode of childbirth: first, the existing data set was enriched and second, previous medical records about the mode of delivery were investigated using machine learning algorithms and by extracting meaningful insights from unseen cases. Several prediction models were trained to achieve this objective, such as decision tree, random forest, {AdaBoostM}1, bagging, and k-nearest neighbor, based on original and enriched data sets.
{RESULTS}: The prediction models based on enriched data performed well in terms of accuracy, sensitivity, specificity, F-measure, and receiver operating characteristic curves in the outcomes. Specifically, the accuracy of k-nearest neighbor was 84.38\%, that of bagging was 83.75\%, that of random forest was 83.13\%, that of decision tree was 81.25\%, and that of {AdaBoostM}1 was 80.63\%. Enrichment of the data set had a good impact on improving the accuracy of the prediction process, which supports maternity care practitioners in making decisions in critical cases.
{CONCLUSIONS}: Our study shows that enriching the data set improves the accuracy of the prediction process, thereby supporting maternity care practitioners in making informed decisions in critical cases. The enriched data set used in this study yields good results, but this data set can become even better if the records are increased with real clinical data.},
	pages = {e28856},
	number = {6},
	journal = {Journal of Medical Internet Research},
	shortjournal = {J Med Internet Res},
	author = {Ullah, Zahid and Saleem, Farrukh and Jamjoom, Mona and Fakieh, Bahjat},
	year = 2021,
	pmid = {34085938},
	pmcid = {PMC8214183},
	keywords = {Humans, Female, Artificial Intelligence, delivery, machine learning, health care, Machine Learning, Pregnancy, {ROC} Curve, decision making, 10, Parturition, cesarean, Maternal Health Services, prediction model},
	file = {Ullah et al_2021_Reliable Prediction Models Based on Enriched Data for Identifying the Mode of.pdf:/Users/joaoalmeida/Zotero/storage/EI8G48SF/Ullah et al_2021_Reliable Prediction Models Based on Enriched Data for Identifying the Mode of.pdf:application/pdf},
