	keywords = {archived, \_tablet},
	pages = {139--144},
	annote = {Version URL: {\textless}a href="https://web.archive.org/web/20210328130302/https://arxiv.org/abs/1406.2661" data-originalurl="http://arxiv.org/abs/1406.2661" data-versiondate="2021-03-28T13:03:02Z"{\textgreater}Robust Link for: http://arxiv.org/abs/1406.2661{\textless}/a{\textgreater}},
	file = {Goodfellow et al_2014_Generative Adversarial Networks.pdf:C\:\\Users\\joaof\\Zotero\\storage\\V524YIGV\\Goodfellow et al_2014_Generative Adversarial Networks.pdf:application/pdf},
